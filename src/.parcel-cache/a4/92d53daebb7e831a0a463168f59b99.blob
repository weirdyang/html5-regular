:root {
  --brand: hsl(172, 100%, 55%);
  --brand-hue: 172;
  --brand-saturation: 100%;
  --brand-lightness: 55%;
  --brand-light: hsl(var(--brand-hue)
  var(--brand-saturation)
  var(--brand-lightness));
  --text1-light: hsl(var(--brand-hue) var(--brand-saturation) 10%);
  --text2-light: hsl(var(--brand-hue) 30% 30%);
  --surface1-light: hsl(var(--brand-hue) 25% 90%);
  --surface2-light: hsl(var(--brand-hue) 20% 99%);
  --surface3-light: hsl(var(--brand-hue) 20% 92%);
  --surface4-light: hsl(var(--brand-hue) 20% 85%);
  --surface-shadow-light: var(--brand-hue) 10% 20%;
  --shadow-strength-light: .2;
  --brand-dark: hsl(
  var(--brand-hue)
  calc(var(--brand-saturation) / 2)
  calc(var(--brand-lightness) / 1.5));
  --text1-dark: hsl(var(--brand-hue) 15% 85%);
  --text2-dark: hsl(var(--brand-hue) 5% 65%);
  --surface1-dark: hsl(var(--brand-hue) 10% 10%);
  --surface2-dark: hsl(var(--brand-hue) 10% 15%);
  --surface3-dark: hsl(var(--brand-hue) 5% 20%);
  --surface4-dark: hsl(var(--brand-hue) 5% 25%);
  --surface-shadow-dark: var(--brand-hue) 50% 3%;
  --shadow-strength-dark: .8;
  --brand-dim: hsl(
  var(--brand-hue)
  calc(var(--brand-saturation) / 1.25)
  calc(var(--brand-lightness) / 1.25));
  --text1-dim: hsl(var(--brand-hue) 15% 75%);
  --text2-dim: hsl(var(--brand-hue) 10% 61%);
  --surface1-dim: hsl(var(--brand-hue) 10% 20%);
  --surface2-dim: hsl(var(--brand-hue) 10% 25%);
  --surface3-dim: hsl(var(--brand-hue) 5% 30%);
  --surface4-dim: hsl(var(--brand-hue) 5% 35%);
  --surface-shadow-dim: var(--brand-hue) 30% 13%;
  --shadow-strength-dim: .2;
  --error: #b00020;
  --on-error: #($color-text-default);
  --br: 0.25rem;
  --space-xxs: .25rem;
  --space-xs: .5rem;
  --space-sm: 1rem;
  --space-md: 1.5rem;
  --space-lg: 2rem;
  --space-xl: 3rem;
  --space-xxl: 6rem;
  --opacity-hover: 0.5;
  --shadow-base: 0 0 0;
  --shadow-1: rgb(var(--shadow-base)/ 26%);
  --shadow-2: rgb(var(--shadow-base) / 28%);
  --shadow-3: rgb(var(--shadow-base) / 22%);
  --min-card-width: 150px;
  background-color: var(--surface1);
  color: var(--text1);
  font-family: system-ui, sans-serif;
  font-size: 1.2rem;
  letter-spacing: 0.01rem;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  text-rendering: optimizeLegibility;
}

html.color-theme-in-transition,
html.color-theme-in-transition *,
html.color-theme-in-transition *:before,
html.color-theme-in-transition *:after {
  transition: background 750ms !important;
  transition-delay: 0 !important;
}

body, html {
  width: 100%;
}
body *, html * {
  border-radius: var(--br);
}

[data-theme=light] {
  color-scheme: light;
  --brand: var(--brand-light);
  --text1: var(--text1-light);
  --text2: var(--text2-light);
  --surface1: var(--surface1-light);
  --surface2: var(--surface2-light);
  --surface3: var(--surface3-light);
  --surface4: var(--surface4-light);
  --surface-shadow: var(--surface-shadow-light);
  --shadow-strength: var(--shadow-strength-light);
}

[data-theme=dark] {
  color-scheme: dark;
  --brand: var(--brand-dark);
  --text1: var(--text1-dark);
  --text2: var(--text2-dark);
  --surface1: var(--surface1-dark);
  --surface2: var(--surface2-dark);
  --surface3: var(--surface3-dark);
  --surface4: var(--surface4-dark);
  --surface-shadow: var(--surface-shadow-dark);
  --shadow-strength: var(--shadow-strength-dark);
}

[data-theme=dim] {
  color-scheme: dark;
  --brand: var(--brand-dim);
  --text1: var(--text1-dim);
  --text2: var(--text2-dim);
  --surface1: var(--surface1-dim);
  --surface2: var(--surface2-dim);
  --surface3: var(--surface3-dim);
  --surface4: var(--surface4-dim);
  --surface-shadow: var(--surface-shadow-dim);
  --shadow-strength: var(--shadow-strength-dim);
}

.text1 {
  color: var(--text1);
}

p.text1 {
  font-weight: 200;
}

.text2 {
  color: var(--text2);
}

.surface1 {
  background-color: var(--surface1);
  color: var(--text2);
}

.surface2 {
  background-color: var(--surface2);
  color: var(--text2);
}

.surface3 {
  background-color: var(--surface3);
  color: var(--text1);
}

.surface4 {
  background-color: var(--surface4);
  color: var(--text1);
}

h1, h2, h3 {
  margin: 0;
  font-weight: 500;
}

main {
  display: grid;
  gap: var(--space-xl);
  place-content: center;
  padding: var(--space-sm);
  background-color: var(--background);
  color: var(--on-background);
}
@media screen and (min-width: 540px) {
  main {
    padding: var(--space-lg);
  }
}
@media screen and (min-width: 800px) {
  main {
    padding: var(--space-xl);
  }
}
main .container {
  margin: auto;
}

main a {
  color: var(--secondary);
}
main a:hover {
  filter: brightness(120%);
}

legend {
  background-color: var(--surface3);
  padding: var(--space-xxs) var(--space-sm);
  box-shadow: 0 3px 1px -2px var(--shadow-1), 0 2px 2px 0 var(--shadow-2), 0 1px 5px 0 var(--shadow-3);
}

fieldset {
  border: 1px solid var(--primary);
  padding: var(--space-xs);
  box-shadow: 0 3px 1px -2px var(--shadow-1), 0 2px 2px 0 var(--shadow-2), 0 1px 5px 0 var(--shadow-3);
}
fieldset input {
  margin: var(--space-xs);
}
fieldset:focus-within {
  background: var(--surface2);
}
fieldset:focus-within legend {
  background: var(--surface2);
}

.rad-shadow {
  box-shadow: 0 3.3px 5px hsl(var(--surface-shadow)/calc(var(--shadow-strength) - 0.184)), 0 11.2px 16.8px hsl(var(--surface-shadow)/calc(var(--shadow-strength) - 0.176)), 0 50px 75px hsl(var(--surface-shadow)/var(--shadow-strength));
}

.navbar {
  height: 3.25rem;
  padding: var(--space-xxs) 0;
  margin: 0;
  width: 100%;
  top: 0;
  position: sticky;
  display: grid;
  grid-template-columns: auto 1fr auto;
  place-content: center;
  border-radius: 0%;
  box-shadow: 0 2px 5px var(--shadow-1);
  color: CanvasText;
}
.navbar a {
  color: CanvasText;
  text-decoration: none;
}
.navbar a:hover {
  text-decoration: underline;
}
.navbar .navbar-start {
  padding-left: var(--space-xxs);
}
.navbar .navbar-end {
  padding-right: var(--space-xxs);
}
.navbar-brand {
  color: var(--brand);
  background-color: var(--brand);
}
.navbar-brand a {
  color: var(--on-primary);
}
.navbar .navbar-items {
  display: grid;
  grid-template-columns: repeat(auto-fit, minmax(150px, 1fr));
  place-content: center;
}
.navbar .navbar-items .link:first-child {
  margin-left: var(--space-sm);
}
.navbar .navbar-items .link:last-child {
  margin-right: var(--space-sm);
}
.navbar .navbar-items .link {
  display: flex;
}
.navbar .navbar-items .link :hover {
  filter: brightness(120%);
}
.navbar .navbar-items .link * {
  margin: auto;
}
@media screen and (max-width: 800px) {
  .navbar .navbar-items .link.is-desktop {
    display: none;
  }
}
@media screen and (max-width: 800px) {
  .navbar .navbar-items {
    display: flex;
    justify-content: space-around;
  }
  .navbar .navbar-items .link:first-child {
    margin-left: 0;
  }
  .navbar .navbar-items .link:last-child {
    margin-right: 0;
  }
}

input {
  border-radius: var(--br);
  padding: var(--space-xxs);
}

input[type=search], input[type=text] {
  background-color: var(--background);
  color: var(--on-background);
}

::placeholder {
  color: var(--placeholder-text);
}

input:focus {
  outline: none;
}

select {
  width: 100%;
  height: 3rem;
  margin: var(--space-sm) 0;
}

.link {
  display: inline-block;
}

.button {
  display: inline-block;
  border: 1px solid transparent;
  text-decoration: none;
  font-size: 1rem;
  cursor: pointer;
  text-align: center;
  border-radius: var(--br);
  padding: var(--space-xxs) var(--space-sm);
  margin: var(--space-xxs);
  background-color: var(--surface3);
  border-color: var(--surface4);
  -webkit-appearance: none;
  -moz-appearance: none;
}
.button:hover {
  box-shadow: 0px 0px 0px 1px var(--surface4);
  box-shadow: inset 0 0 0 10em rgba(255, 255, 255, 0.3);
}
.button:active {
  transform: scale(0.99);
}
.button.circle {
  display: flex;
  max-width: 2em;
  border-radius: 50%;
  background-color: transparent;
  text-align: center;
  justify-content: center;
  box-shadow: 0 3px 1px -2px var(--shadow-1), 0 2px 2px 0 var(--shadow-2), 0 1px 5px 0 var(--shadow-3);
}
.button.circle span {
  margin: 0;
  padding-bottom: 1px;
}
.button-outlined {
  border-style: solid;
  border-width: 1px;
  border-color: CanvasText;
  background-color: transparent;
  color: var(--text1);
}
.button-outlined:hover {
  box-shadow: inset 0 0 0 10em rgba(255, 255, 255, 0.3);
}
.button-raised {
  border: none;
  background-color: transparent;
  box-shadow: 0 3px 1px -2px var(--shadow-1), 0 2px 2px 0 var(--shadow-2), 0 1px 5px 0 var(--shadow-3);
}
.button-raised:hover {
  box-shadow: 0 3px 1px -2px var(--shadow-1), 0 2px 2px 0 var(--shadow-2), 0 1px 5px 0 var(--shadow-3);
  background-color: var(--surface2);
  color: var(--text2);
}

.card {
  display: flex;
  flex-direction: column;
  padding: 1rem;
  justify-content: space-between;
}
.card h3 {
  margin: 0;
}
.card .card-header {
  box-shadow: 0 3px 1px -2px var(--shadow-1), 0 2px 2px 0 var(--shadow-2), 0 1px 5px 0 var(--shadow-3);
  padding: var(--space-xxs) var(--space-sm);
}
.card .card-content {
  display: flex;
  flex-direction: column;
}
.card .card-text {
  border: 1px solid var(--surface4);
  margin: var(--space-xxs) 0;
  padding: 0 var(--space-sm);
  max-height: 8rem;
  overflow-y: scroll;
  overflow-x: hidden;
}
.card .card-text::-webkit-scrollbar {
  width: 0.5em;
}
.card .card-text::-webkit-scrollbar-track {
  background-color: var(--shadow-3);
  opacity: 0.2;
}
.card .card-text::-webkit-scrollbar-thumb {
  background-color: var(--surface4);
  outline: 0.25rem var(--surface4);
}
.card .card-footer {
  margin-top: var(--space-xs);
}
.card .card-footer * {
  width: 100%;
}
.card figure {
  display: flex;
  justify-content: center;
}
.card .visual {
  height: 8rem;
  min-height: 100px;
  min-width: var(--min-card-width);
  object-fit: contain;
}

.cards-group {
  display: grid;
  gap: 1.25rem;
  row-gap: 1.5rem;
  grid-template-columns: repeat(auto-fit, minmax(var(--min-card-width), 1fr));
}